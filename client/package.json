{
  "name": "onestop-client",
  "version": "2.4.2",
  "description": "The web client for the OneStop discovery service",
  "repository": "ssh://git@github.com:cedardevs/onestop.git",
  "license": "GPL-2.0",
  "scripts": {
    "dev": "webpack serve --hot --mode development --env URL_API_SEARCH=http://localhost:8097/ --progress --config webpack.config.js --color",
    "kub": "webpack serve --host 0.0.0.0 --hot --mode development --env URL_API_GATEWAY=http://localhost/ --progress --config webpack.config.js --color",
    "format": "find src test -name '*.js' -o -name '*.jsx' -o -name '*.css' | xargs ./node_modules/.bin/prettier --config .prettierrc.json --write",
    "formatCheck": "find src test -name '*.js' -o -name '*.jsx' -o -name '*.css' | xargs ./node_modules/.bin/prettier --config .prettierrc.json --list-different",
    "test": "jest --maxWorkers=2 --config jest.config.js",
    "testFresh": "jest --no-cache --config jest.config.js",
    "build": "npm install && NODE_ENV=production ./node_modules/.bin/webpack --mode production",
    "clean": "rm -rf node_modules && rm -rf build && rm -rf coverage",
    "retire": "retire -p",
    "retireGitLabCICD": "retire -p --jsrepo https://artifacts.ncei.noaa.gov/artifactory/jsretire-repository/jsrepository.json --noderepo https://artifacts.ncei.noaa.gov/artifactory/jsretire-repository/npmrepository.json --cacert /etc/pki/ca-trust/extracted/pem/tls-ca-bundle.pem",
    "dependencies": "npm list -depth=8",
    "auditFix": "npm audit fix --force",
    "audit": "npm audit --trace-deprecation",
    "updateBrowsers": "echo updating browsers;npx browserslist@latest --update-db"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.3",
    "connected-react-router": "^6.9.2",
    "detect-browser": "^5.1.0",
    "esri-leaflet": "^3.0.4",
    "esri-loader": "^3.4.0",
    "file-saver": "^2.0.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^3.0.0",
    "leaflet": "^1.7.1",
    "leaflet-draw": "^1.0.4",
    "lodash": "^4.17.19",
    "moment": "^2.29.4",
    "npm": "^8.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-animate-height": "^2.0.7",
    "react-aria-live": "^2.0.4",
    "react-browser-detection": "^1.2.1",
    "react-dom": "^16.7.0",
    "react-ga4": "^2.1.0",
    "react-grid-system": "^4.4.1",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "react-spring": "8.0.4",
    "react-toggle-display": "^2.2.0",
    "redux": "^4.0.1",
    "redux-seamless-immutable": "^0.4.0",
    "redux-thunk": "^2.3.0",
    "redux-watch": "^1.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "retire": "^3.0.7",
    "seamless-immutable": "^7.1.4"
  },

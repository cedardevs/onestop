{
  "name": "onestop-client",
  "version": "2.0.0-RC9",
  "description": "The web client for the OneStop discovery service",
  "repository": "ssh://git@github.com:cedardevs/onestop.git",
  "license": "GPL-2.0",
  "scripts": {
    "dev": "webpack-dev-server --env.URL_API_SEARCH=http://localhost:8097 --progress --config webpack.config.js --colors & npm run testAndWatch",
    "kub": "webpack-dev-server --env.URL_API_SEARCH=$(minikube service onestop-api-search --url) --progress --config webpack.config.js --colors & npm run testAndWatch",
    "test": "NODE_ENV=test mocha --recursive ./test",
    "testAndWatch": "NODE_ENV=test mocha --recursive ./test -w --watch-extensions js,jsx",
    "format": "find src -name '*.js' -o -name '*.jsx' -o -name '*.css' | xargs ./node_modules/.bin/prettier --config .prettierrc.json --write",
    "build": "npm install && npm test && NODE_ENV=production ./node_modules/.bin/webpack -p",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^4.3.0",
    "esri-leaflet": "^2.1.2",
    "esri-loader": "^1.5.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^1.3.1",
    "leaflet-draw": "^0.4.13",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "npm": "^5.8.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-animate-height": "^1.0.1",
    "react-browser-detection": "^1.1.1",
    "react-dom": "^16.4.0",
    "react-grid-system": "^3.1.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-toggle-display": "^2.2.0",
    "redux": "^4.0.0",
    "redux-seamless-immutable": "^0.4.0",
    "redux-thunk": "^2.2.0",
    "redux-watch": "^1.1.1",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.1.0",
    "json-loader": "^0.5.7",
    "mocha": "^4.0.1",
    "modernizr": "^3.5.0",
    "modernizr-loader": "^1.0.1",
    "nock": "^9.1.0",
    "path": "^0.12.7",
    "postcss-assets": "^5.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "prettier-miscellaneous": "^1.5.3",
    "purecss": "^1.0.0",
    "react-css-modules": "^4.7.1",
    "redux-mock-store": "^1.3.0",
    "resolve-url-loader": "^2.3.0",
    "sinon": "^4.1.2",
    "speed-measure-webpack-plugin": "^1.2.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.11.1"
  }
}

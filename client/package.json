{
  "name": "onestop-client",
  "version": "2.4.2",
  "description": "The web client for the OneStop discovery service",
  "repository": "ssh://git@github.com:cedardevs/onestop.git",
  "license": "GPL-2.0",
  "scripts": {
    "dev": "webpack-dev-server --hot --mode development --env.URL_API_SEARCH=http://localhost:8097/ --progress --config webpack.config.js --colors",
    "kub": "webpack-dev-server --host 0.0.0.0 --hot --mode development --env.URL_API_SEARCH=http://localhost:30097/ --env.URL_API_GATEWAY=http://localhost/ --progress --config webpack.config.js --colors",
    "format": "find -f src test -name '*.js' -o -name '*.jsx' -o -name '*.css' | xargs ./node_modules/.bin/prettier --config .prettierrc.json --write",
    "formatCheck": "find -f src test -name '*.js' -o -name '*.jsx' -o -name '*.css' | xargs ./node_modules/.bin/prettier --config .prettierrc.json --list-different",
    "test": "jest --maxWorkers=2 --config jest.config.js",
    "testFresh": "jest --no-cache --config jest.config.js",
    "build": "npm install && NODE_ENV=production ./node_modules/.bin/webpack --mode production -p",
    "clean": "rm -rf build && rm -rf coverage",
    "retire": "retire -p"
  },

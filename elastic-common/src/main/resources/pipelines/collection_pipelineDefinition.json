{
  "description": "[RELEASE-DATE] Transforms and reduces fields from staging_collection to search_collection",
  "version": 1,
  "processors": [
    {
      "foreach": {
        "field": "keywords",
        "processor": {
          "foreach": {
            "field": "_ingest._value.values",
            "processor": {
              "append": {
                "field": "keywordValues",
                "value": [
                  "{{_ingest._value}}"
                ]
              }
            },
            "on_failure": [
              {
                "append": {
                  "field": "keywordValues",
                  "value": [
                    "{{_ingest._value.values}}"
                  ]
                }
              }
            ]
          }
        },
        "on_failure": [
          {
            "append": {
              "field": "keywordValues",
              "value": [
                "{{keywords.values}}"
              ]
            }
          }
        ]
      }
    },
    {
      "foreach": {
        "field": "contacts",
        "processor": {
          "append": {
            "field": "individualNames",
            "value": "{{_ingest._value.individualName}}"
          }
        },
        "on_failure": [
          {
            "append": {
              "field": "individualNames",
              "value": "{{contacts.individualName}}"
            }
          }
        ]
      }
    },
    {
      "foreach": {
        "field": "contacts",
        "processor": {
          "append": {
            "field": "organizationNames",
            "value": "{{_ingest._value.organizationName}}"
          }
        },
        "on_failure": [
          {
            "append": {
              "field": "organizationNames",
              "value": "{{contacts.organizationName}}"
            }
          }
        ]
      }
    },
    {
      "foreach": {
        "field": "creators",
        "processor": {
          "append": {
            "field": "individualNames",
            "value": "{{_ingest._value.individualName}}"
          }
        },
        "on_failure": [
          {
            "append": {
              "field": "individualNames",
              "value": "{{creators.individualName}}"
            }
          }
        ]
      }
    },
    {
      "foreach": {
        "field": "creators",
        "processor": {
          "append": {
            "field": "organizationNames",
            "value": "{{_ingest._value.organizationName}}"
          }
        },
        "on_failure": [
          {
            "append": {
              "field": "organizationNames",
              "value": "{{creators.organizationName}}"
            }
          }
        ]
      }
    },
    {
      "foreach": {
        "field": "publishers",
        "processor": {
          "append": {
            "field": "individualNames",
            "value": "{{_ingest._value.individualName}}"
          }
        },
        "on_failure": [
          {
            "append": {
              "field": "individualNames",
              "value": "{{publishers.individualName}}"
            }
          }
        ]
      }
    },
    {
      "foreach": {
        "field": "publishers",
        "processor": {
          "append": {
            "field": "organizationNames",
            "value": "{{_ingest._value.organizationName}}"
          }
        },
        "on_failure": [
          {
            "append": {
              "field": "organizationNames",
              "value": "{{publishers.organizationName}}"
            }
          }
        ]
      }
    },
    {
      "date": {
        "field": "temporalBounding.beginDate",
        "target_field": "beginDate",
        "formats": ["ISO8601"],
        "ignore_failure": true
      }
    },
    {
      "set": {
        "field": "beginYear",
        "value": "{{temporalBounding.beginYear}}"
      }
    },
    {
      "convert": {
        "field": "beginYear",
        "type": "auto"
      }
    },
    {
      "date": {
        "field": "temporalBounding.endDate",
        "target_field": "endDate",
        "formats": ["ISO8601"],
        "ignore_failure": true
      }
    },
    {
      "set": {
        "field": "endYear",
        "value": "{{temporalBounding.endYear}}"
      }
    },
    {
      "convert": {
        "field": "endYear",
        "type": "auto"
      }
    },
    {
      "remove": {
        "field": "purpose"
      }
    },
    {
      "remove": {
        "field": "status"
      }
    },
    {
      "remove": {
        "field": "credit"
      }
    },
    {
      "remove": {
        "field": "hierarchyLevelName"
      }
    },
    {

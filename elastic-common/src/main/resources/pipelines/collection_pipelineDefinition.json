      }
    },
    {
      "remove": {
        "field": "dsmmDataQualityAssessment"
      }
    },
    {
      "remove": {
        "field": "dsmmDataQualityAssurance"
      }
    },
    {
      "remove": {
        "field": "dsmmDataQualityControlMonitoring"
      }
    },
    {
      "remove": {
        "field": "dsmmPreservability"
      }
